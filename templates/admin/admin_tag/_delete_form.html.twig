<form method="post" action="{{ path('app_admin_tag_delete', {'id': tag.id}) }}"
      onsubmit="return confirm('Etes-vous sur de vouloir supprimer ce tag?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tag.id) }}">
    <button class="rounded-2">Supprimer</button>
</form>
