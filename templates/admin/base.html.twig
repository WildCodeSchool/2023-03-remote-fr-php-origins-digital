<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/png" href="{{ asset('build/images/logo_origins.png') }}"/>
    <script src="https://kit.fontawesome.com/848d5c22f9.js" crossorigin="anonymous"></script>
    <title>
        {% block title %}Origin Digital
        {% endblock %}
    </title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('intro') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('intro') }}
    {% endblock %}
</head>

<body class="bg-gradient-reversed">
{% include '_includes/_navbar.html.twig' %}
<div class="container mt-5">
    <div class="row">
            {% include '_includes/_flash.html.twig' %}

        {% include "/admin/_partials/adminnav.html.twig" %}

        <main class="col-9 border border-light p-2 text-secondary">
            {% block body %}{% endblock %}
        </main>
    </div>
</div>
</body>
</html>
